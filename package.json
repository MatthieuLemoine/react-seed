{
  "name": "react-seed",
  "version": "1.0.0",
  "description": "A React seed using Redux built with Webpack",
  "main": "src/index.js",
  "scripts": {
    "build": "./node_modules/webpack/bin/webpack.js --config webpack.config.js --progress --colors",
    "watch": "./node_modules/webpack/bin/webpack.js --config webpack.config.js --progress --colors --watch",
    "prod": "./node_modules/webpack/bin/webpack.js --config webpack.config.prod.js --progress --colors",
    "lint": "./node_modules/eslint/bin/eslint.js ./src --ext .js --ignore-path .gitignore --cache",
    "check": "npm run lint && npm run prod"
  },
  "author": "MatthieuLemoine",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.16.0",
    "babel-loader": "^6.2.5",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-1": "^6.16.0",
    "eslint": "^3.7.0",
    "eslint-config-airbnb": "^12.0.0",
    "eslint-plugin-import": "^1.16.0",
    "eslint-plugin-jsx-a11y": "^2.2.2",
    "eslint-plugin-react": "^6.3.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "html-loader": "^0.4.4",
    "html-webpack-plugin": "^2.22.0",
    "json-loader": "^0.5.4",
    "jsx-loader": "^0.13.2",
    "style-loader": "^0.13.1",
    "webpack": "^1.13.2"
  },
  "dependencies": {
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "react-redux": "^4.4.5",
    "redux": "^3.6.0",
    "redux-logger": "^2.6.1",
    "redux-thunk": "^2.1.0"
  }
}
