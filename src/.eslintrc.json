{
  "globals" : {
    "React" : true
  },
  "env": {
    "browser": true
  },
  "extends": "airbnb",
  "rules" :{
      "no-multi-spaces"  : 0,
      "key-spacing" : [1,{
        "beforeColon" : true,
        "afterColon"  : true,
        "mode"        : "minimum"
      }],
      "no-use-before-define" : 0,
      "no-param-reassign" : 0,
      "comma-dangle" : 0,
      "react/react-in-jsx-scope" : 0,
      "react/jsx-filename-extension" : 0,
      "react/forbid-prop-types" : 0,
      "import/extensions" : 0,
      "new-cap" : 0,
      "no-confusing-arrow" : 0,
      "arrow-parens" : 0,
      "no-console" : 0,
      "no-underscore-dangle" : 0
    }
}
